.dl-menuwrapper {
  	{% if settings.nav-style == 'mobile' %}{% elsif settings.nav-style == 'full' %}display: none;{% endif %}
	width: 100%;
	max-width: 300px;
  	z-index: 5;
	float: left;
  	top: 40px;
  	left: 10px;
	position: fixed;
	-webkit-perspective: 1000px;
	-moz-perspective: 1000px;
	perspective: 1000px;
	-webkit-perspective-origin: 50% 200%;
	-moz-perspective-origin: 50% 200%;
	perspective-origin: 50% 200%;
}

.dl-menuwrapper:first-child {
	margin-right: 100px;
}

.dl-menuwrapper button {
	background: {{ settings.navigation }};
	border: none;
	width: 48px;
	height: 45px;
    font-size: 13px;
  	text-align: center;
	/*text-indent: -900em; */
	overflow: hidden;
  	padding: 0;
	position: relative;
	cursor: pointer;
	outline: none;
    margin-bottom: 0;
  	color: {{ settings.nav_color }};
}

.dl-menuwrapper button:hover,
.dl-menuwrapper button.dl-active,
.dl-menuwrapper ul {
  background: {{ settings.navigation }};
}

.dl-menuwrapper button:after {
	content: '';
	position: absolute;
	width: 68%;
	height: 5px;
  	/*background: {{ settings.nav_color }};*/
	top: 10px;
	left: 16%;	
  	/* box-shadow: 
		0 10px 0 {{ settings.nav_color }}, 
		0 20px 0 {{ settings.nav_color }}; */
}

.dl-menuwrapper ul {
	padding: 0;
	list-style: none;
	-webkit-transform-style: preserve-3d;
	-moz-transform-style: preserve-3d;
	transform-style: preserve-3d;
}

.dl-menuwrapper li {
	position: relative;
}

.dl-menuwrapper li a {
  font-family: {{ settings.navigation-font }};
	display: block;
	position: relative;
	padding: 15px 20px;
	font-size: {{ settings.nav_size }};
	line-height: 20px;
  font-weight: {{ settings.nav-weight }};
    color: {{ settings.nav_color }};
	outline: none;
  	text-transform: {{ settings.navigation-transform }};
}

.no-touch .dl-menuwrapper li a:hover {
  background: {{ settings.nav_hover_color }};
  color: {{ settings.nav_hover_link_color }};
}

.dl-menuwrapper li.dl-back > a {
	padding-left: 30px;
  	background: {{ settings.navigation }};
}

.dl-menuwrapper li.dl-back:after,
.dl-menuwrapper li > a:not(:only-child):after {
	position: absolute;
	top: 0;
	line-height: 50px;
	font-family: 'FontAwesome';
	speak: none;
  	color: {{ settings.nav_color }};
	-webkit-font-smoothing: antialiased;
	content: "\f054";
}

.dl-menuwrapper li.dl-back:after {
	left: 10px;
